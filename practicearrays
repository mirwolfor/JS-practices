var names = ["Rock","Paper","Scissors"]
var userChoice = prompt("Select a number from 1 to 3");

if(userChoice === "1"){
userChoice = names[0];
} 
else if(userChoice === "2"){
    userChoice =  names[1];
}
else if(userChoice === "3"){
    userChoice =  names[2];
}
else {
   userChoice = prompt("Whoops! It appears you have made an invalid selection.");
}
console.log(userChoice);
